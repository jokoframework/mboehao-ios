# Uncomment the next line to define a global platform for your projecti
 platform :ios, '10.0'

target 'iOS Starter Kit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS Starter Kit
pod 'Alamofire'
pod 'Firebase/Core'
pod 'Firebase/Messaging'
pod 'Firebase/Auth'
pod 'GoogleSignIn'
pod 'SVProgressHUD'
pod 'ActionSheetPicker-3.0'
pod 'SwiftyJSON'
pod 'SwiftLint'
pod 'FBSDKLoginKit'
pod 'SideMenu'
 
end
